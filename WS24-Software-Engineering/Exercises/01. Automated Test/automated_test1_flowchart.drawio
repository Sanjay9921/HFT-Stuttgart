        <mxCell id="3_n_T43A4tmUcUw6OImN-20" value="&lt;b&gt;ASSEMBLY&lt;/b&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
